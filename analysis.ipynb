   "source": [
    "import os\n",
    "import pandas as pd\n",
    "\n",
    "dfs = []\n",
    "for filename in os.listdir('data'):\n",
    "    path = os.path.join('data', filename)\n",
    "    if os.path.isfile(path):\n",
    "        df = pd.read_csv(path)\n",
    "        if len(df) >= 999:\n",
    "            dfs.append(df)\n",
    "\n",
    "df = pd.concat(dfs, ignore_index=True)"
   ]
  }
